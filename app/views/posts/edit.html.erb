<div class="card my-3">
  <div class="card-header">
    <h2>Editing Post - #<%= link_to @post.id, @post %></h2>
  </div>
  <div class="card-body">
    <%= render 'form', post: @post %>
  </div>
  <div class="card-footer">
    <%= link_to 'Back', :back, class: "btn btn-secondary" %>
  </div>
</div>


